<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.match.springmvc.entities.Bonusscale" table="bonusscale">
        <id name="Id" type="java.lang.Integer">
            <column name="Id" />
            <generator class="identity" />
        </id>
        <property name="Maxnumber" type="java.lang.Integer">
            <column name="Maxnumber" />
        </property>
        <property name="Minnumber" type="java.lang.Integer">
            <column name="Minnumber" />
        </property>
        <property name="Scale" type="java.lang.Double">
            <column name="Scale" />
        </property>
    </class>
</hibernate-mapping>
