<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.match.springmvc.entities.Admin" table="admin">
        <id name="Adid" type="java.lang.String">
            <column name="Adid" />
            <generator class="assigned" />
        </id>
        <property name="Adpwd" type="java.lang.String">
            <column name="Adpwd" />
        </property>
    </class>
</hibernate-mapping>
